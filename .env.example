# Environment Template
# Copia este archivo como .env y configura tus valores

# Base de Datos PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_NAME=monitoreo_ciudadano
DB_USER=postgres
DB_PASSWORD=tu_password_aqui

# APIs Externas (requeridas)
GOOGLE_MAPS_API_KEY=TU_GOOGLE_MAPS_API_KEY_AQUI
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=TU_GOOGLE_MAPS_API_KEY_AQUI

# OAuth Configuration
OAUTH_GOOGLE_CLIENT_ID=TU_GOOGLE_CLIENT_ID_AQUI
OAUTH_FACEBOOK_APP_ID=TU_FACEBOOK_APP_ID_AQUI

# App Configuration
EXPO_PUBLIC_API_URL=http://localhost:3000/api
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_APP_NAME=Monitoreo Ciudadano

# Notificaciones
EXPO_PUSH_TOKEN=TU_EXPO_PUSH_TOKEN_AQUI

# Desarrollo
NODE_ENV=development
